   "source": [
    "def timecon(s):\n",
    "    meridian=s[-2:]\n",
    "    if meridian==\"AM\" and s[:2]==\"12\":\n",
    "        s=\"00\"+s[2:]\n",
    "    elif meridian==\"PM\" and s[:2]!=\"12\":\n",
    "        s=str(12 + int(s[:2]))+ s[2:]\n",
    "    return s[:-2]    "
   ]
