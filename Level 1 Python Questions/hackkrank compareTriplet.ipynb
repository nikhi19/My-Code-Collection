    "def comtri(a,b):\n",
    "    alice=bob=0\n",
    "    for i in range(3):\n",
    "        if a[i]>b[i]:\n",
    "            alice+=1\n",
    "        elif a[i]<b[i]:\n",
    "            bob+=1\n",
    "    return alice,bob        "
   ]
